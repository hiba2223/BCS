class Rectangle{
  public static void main(String args[]){
    int height, width; 
    height = 4; 
    width = 3; 
    int area = 4*3; 
    int circumference = 4+4+3+3;
    System.out.println("Height: " + height);
    System.out.println("Width: " + width);
    System.out.println("Area: " + area);
    System.out.println("circumference: " + circumference);
    }
  }
